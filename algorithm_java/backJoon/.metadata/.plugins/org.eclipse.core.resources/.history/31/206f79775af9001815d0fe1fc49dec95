import java.util.Arrays;
import java.util.Collections;
import java.util.HashSet;
import java.util.List;
import java.util.Scanner;
import java.util.Set;

public class test_9933 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		short lines = (short)scan.nextInt();
		
		Set<String> texts = new HashSet<String>();
		
		scan.nextLine();

		List reverses;
		
		for(int i=0; i<lines; i++) {
			String input = scan.nextLine();
			texts.add(input);
			reverses = Arrays.asList(input);
			Collections.reverse(reverses);
			String reverse = (reverses.toString()).replaceAll("[^a-zA-Z]", "");
			if(texts.contains(reverse)) {
				int idx = reverse.length();
				System.out.print(idx +" "+ reverse.charAt(idx/2));
				break;
			}
		}
		
		scan.close();
	}

}
