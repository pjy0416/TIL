import java.util.Arrays;
import java.util.HashSet;
import java.util.Scanner;
import java.util.Set;

public class test_9933 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		short lines = (short)scan.nextInt();
		
		Set<String> texts = new HashSet<String>();
		
		scan.nextLine();

		StringBuffer buffer = new StringBuffer();
	      strBuffer.append(str);
	      // 버퍼안에 reverse()를 이용해 거꾸로 출력
	      System.out.print(strBuffer.reverse());
		
		
		for(int i=0; i<lines; i++) {
			String input = scan.nextLine();
			texts.add(input);
			buffer.append(texts);
			String reverse = buffer.reverse();
			
			if(texts.contains(reverse)) {
				int idx = reverse.length();
				System.out.print(idx +" "+ reverse.charAt(idx/2));
				break;
			}
		}
		
		scan.close();
	}

}
