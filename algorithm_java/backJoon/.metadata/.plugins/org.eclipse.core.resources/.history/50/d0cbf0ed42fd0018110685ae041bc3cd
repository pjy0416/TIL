import java.util.Scanner;

public class test_2168 {
	public static long gcd(long a,long b){
		while(true) {
			long tmp = a;
			a = b;
			b %= tmp;
			
			if(b==0) {
				break;
			}
		}
		return a;
	}
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in); 
		long n = sc.nextLong(); 
		long m = sc.nextLong(); 
		System.out.println(n+m-gcd(n,m));  
	}
	
	
	//
}
