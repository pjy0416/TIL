import java.util.ArrayList;
import java.util.Collections;
import java.util.Scanner;

public class test_16471 {

	public static void main(String[] args) {
		// player information
		ArrayList<Integer> master = new ArrayList<Integer>();		// 주인
		ArrayList<Integer> guest = new ArrayList<Integer>();	// 손님
		
		// input variance
		Scanner scan = new Scanner(System.in);	
		
		// total cards number, win point
		int numCards = scan.nextInt();	// input a number of cards
		int winPoint = numCards+1;		// win point
		int guestPoint = 0;				// guestPoint
		
		for(int i=0; i<numCards; i++) {	// add a master's cards as total
			guest.add(scan.nextInt());
		}
		
		for(int i=0; i<numCards; i++) {	// add a master's cards as total
			master.add(scan.nextInt());
		}

		// 비교를 위한 내림차 정렬
		Collections.sort(master);
		Collections.sort(guest);
		Collections.reverse(master);
		Collections.reverse(guest);
		
		//temp 변수
		int cardMaster =0;
		int cardGuest =0;
		boolean isEnd = false;
		
		master.remove(1);
		for(int i=0; i<master.size(); i++) {
			System.out.println(master.get(i));
		}
		
		/*for(int i=0; i<numCards; i++) {
			if(guestPoint >= winPoint/2) {
				break;
			}
			
			System.out.println("guest card : " + guest.get(0));
			
			int size = guest.size();
			cardGuest = guest.get(0);	//first card
			
			for(int j=0; j<size; j++) {
				cardMaster = master.get(j);
				System.out.println("master card : " + cardMaster);
				if(cardGuest < cardMaster) {
					guestPoint++;
					master.remove(j);
					guest.remove(0);
					break;
				} else if(j == size-1) {
					master.remove(0);
					guest.remove(0);
				}
			}
			System.out.println("--------------------------------------");
		}*/
		
		if(guestPoint >= winPoint/2) {
			System.out.println("Yes");
		}else {
			System.out.println("No");
		}
	}
}
