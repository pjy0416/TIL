import java.util.Scanner;

public class test_16462 {
	private static int convertInt(String textScore) {
		textScore = textScore.replaceAll("[0]", "9").replaceAll("[6]", "9");
		int result = Integer.parseInt(textScore);
		
		if(result > 100) {
			return 100;
		}
		return result;
	}
	
	private static boolean isNatural(int num, int max) {
		if(num >= 1 || num <=max) {
			return true;
		} else {
			return false;
		}
	}

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		short count = 0;
		
		boolean isNum = false;
		while(!isNum) {
			count = (short)scan.nextInt();
			isNum = isNatural(count, 1000);
		}
		String textScore = "";
		int sum =0;
		
		for(int i=0; i<count; i++) {
			textScore =scan.next();
			sum += convertInt(textScore);
		}
		
		System.out.println(sum/count);
		
		scan.close();
	}

}
